{
	"name": "orange-meets",
	"module": "worker.js",
	"private": true,
	"sideEffects": false,
	"scripts": {
		"build": "remix build",
		"lint": "prettier --check . && eslint --ignore-path .gitignore --cache --cache-location ./node_modules/.cache/eslint .",
		"dev": "remix dev --manual -c \"npm start\"",
		"start": "wrangler dev ./build/index.js",
		"prebuild": "npm run clean",
		"clean": "del public/build && del build",
		"ci-test": "CI=true run-p typecheck test",
		"remove-sourcemaps": "del public/build/**/*.map",
		"deploy": "npm run ci-test && npm run build -- --sourcemap && npm run remove-sourcemaps && wrangler publish --define __RELEASE__:\\\"$(git rev-parse --short HEAD)\\\" --define __SENTRY_DSN__:\\\"$SENTRY_DSN\\\"",
		"prettier": "prettier -w .",
		"typecheck": "tsc --noEmit --skipLibCheck",
		"typecheck:watch": "npm run typecheck -- --watch",
		"test": "vitest",
		"test:ci": "vitest --watch false",
		"check": "npm run lint && npm run typecheck && npm run test:ci"
	},
	"dependencies": {
		"@heroicons/react": "^2.1.4",
		"@mediapipe/selfie_segmentation": "^0.1.1675465747",
		"@radix-ui/react-alert-dialog": "^1.1.1",
		"@radix-ui/react-checkbox": "^1.1.0",
		"@radix-ui/react-dialog": "^1.1.1",
		"@radix-ui/react-dropdown-menu": "^2.1.1",
		"@radix-ui/react-label": "^2.1.0",
		"@radix-ui/react-select": "^2.1.1",
		"@radix-ui/react-slot": "^1.1.0",
		"@radix-ui/react-switch": "^1.1.0",
		"@radix-ui/react-toast": "^1.2.1",
		"@radix-ui/react-tooltip": "^1.1.1",
		"@radix-ui/react-visually-hidden": "^1.1.0",
		"@reactivex/rxjs": "^6.6.7",
		"@remix-run/cloudflare": "2.10.0",
		"@remix-run/react": "2.10.0",
		"@tensorflow-models/body-segmentation": "^1.0.2",
		"@tensorflow/tfjs-backend-webgl": "^4.15.0",
		"@tensorflow/tfjs-core": "^4.15.0",
		"clsx": "^2.1.1",
		"cookie": "^0.6.0",
		"nanoid": "^5.0.7",
		"partyserver": "^0.0.45",
		"partysocket": "^1.0.1",
		"react": "^18.3.1",
		"react-dom": "^18.3.1",
		"react-flip-toolkit": "^7.1.0",
		"react-query": "^3.39.3",
		"react-use": "^17.5.0",
		"rxjs": "^7.8.1",
		"tailwind-merge": "^2.3.0",
		"tiny-invariant": "^1.3.3",
		"webrtc-adapter": "^9.0.1"
	},
	"devDependencies": {
		"@cloudflare/vitest-pool-workers": "^0.4.7",
		"@cloudflare/workers-types": "^4.20240620.0",
		"@peculiar/webcrypto": "^1.5.0",
		"@remix-run/dev": "2.10.0",
		"@remix-run/eslint-config": "2.10.0",
		"@types/react": "^18.3.3",
		"@types/react-dom": "^18.3.0",
		"del-cli": "^5.1.0",
		"eslint": "^8.56.0",
		"git-format-staged": "^3.1.1",
		"msw": "^2.3.1",
		"npm-run-all": "^4.1.5",
		"prettier": "^3.3.2",
		"prettier-plugin-organize-imports": "^3.2.4",
		"tailwindcss": "^3.4.4",
		"typescript": "^5.5.2",
		"vitest": "1.5.0",
		"wrangler": "^3.62.0"
	},
	"overrides": {
		"msw": {
			"typescript": "$typescript"
		},
		"pretty-format": "29.7.0",
		"esbuild": "^0.23.0"
	},
	"engines": {
		"node": ">=16.13"
	},
	"msw": {
		"workerDirectory": "public"
	}
}
